@prefix :         <http://gsb.leipert.io/ns/> .
@prefix gsb: <http://gsb.leipert.io/ns/> .
@prefix doap:     <http://usefulinc.com/ns/doap#>.
@prefix foaf:     <http://xmlns.com/foaf/0.1/> .
@prefix owl:      <http://www.w3.org/2002/07/owl#> .
@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann:     <http://purl.org/vocab/vann/>.
@prefix xsd:      <http://www.w3.org/2001/XMLSchema#> .

<http://gsb.leipert.io/ns/> a owl:Ontology;
    foaf:maker <http://leipert.io>;
    doap:maintainer <http://leipert.io>;
    doap:homepage <http://leipert.io/gsb>;
    vann:changes <https://github.com/leipert/gsb/commits/master/test.ttl>;
    vann:preferredNamespacePrefix "gsb";
    vann:preferredNamespaceUri "http://gsb.leipert.io/ns/";
    rdfs:label "Graphical SPARQL Builder Vocabulary.";
    rdfs:isDefinedBy <> .

# Start Class Agent
:Agent a rdfs:Class ;
    rdfs:label "Agent"@de, "Agent"@en ;
    rdfs:comment "Analog zu foaf:Agent"@de, "Analogue to foaf:Agent"@en ;
    rdfs:subClassOf owl:Thing ;
    owl:equivalentClass foaf:Agent .

    # Agent Properties

    :founder a owl:ObjectProperty, owl:IrreflexiveProperty ;
        rdfs:label "Gr端nder"@de, "Founder"@en ;
        rdfs:comment "Gr端nder einer Organisation"@de, "Founder of an Organisation"@en ;
        rdfs:domain foaf:Agent ;
        rdfs:range :Organisation .

    :owner a owl:ObjectProperty, owl:IrreflexiveProperty ;
        rdfs:label "Eigent端mer"@de, "Owner"@en ;
        rdfs:comment "Eigent端mer einer Organisation"@de, "Owner of an Organisation"@en ;
        rdfs:domain :Agent ;
        rdfs:range foaf:Organization .

# End Class Agent

# Start Class Person
:Person a rdfs:Class ;
    rdfs:label "Person"@de, "Person"@en ;
    rdfs:comment "Analog zu foaf:Person"@de, "Analogue to foaf:Person"@en ;
    rdfs:subClassOf :Agent ;
    owl:equivalentClass foaf:Person .

    # Person Properties

    :ancestor a owl:ObjectProperty, owl:TransitiveProperty, owl:AsymmetricProperty, owl:IrreflexiveProperty ;
        rdfs:label "Vorfahr"@de, "Ancestor"@en ;
        rdfs:comment "Der Vorfahr einer Person"@de, "The Ancestor of a person"@en ;
        rdfs:domain :Person ;
        rdfs:range :Person ;
        owl:inverseOf :scion .

    :scion a owl:ObjectProperty, owl:TransitiveProperty, owl:AsymmetricProperty, owl:IrreflexiveProperty ;
        rdfs:label "Nachfahr"@de, "Scion"@en ;
        rdfs:comment "Der Vorfahr einer Person"@de, "The scion of a person"@en ;
        rdfs:domain foaf:Person ;
        rdfs:range foaf:Person ;
        owl:inverseOf :scion .

# End Class Person

:Organisation a rdfs:Class ;
    rdfs:label "Organisation"@de, "Organisation"@en ;
    rdfs:comment "Analog zu foaf:Organization"@de, "Analogue to foaf:Organisation"@en ;
    rdfs:subClassOf :Agent ;
    owl:equivalentClass foaf:Organization .
